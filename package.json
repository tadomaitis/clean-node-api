  "license": "MIT",
  "devDependencies": {
    "@shelf/jest-mongodb": "4.1.7",
    "husky": "8.0.3",
    "jest": "29.7.0",
    "lint-staged": "15.0.2",
    "standard": "17.1.0",
    "supertest": "6.3.3"
  },
  "dependencies": {
    "bcrypt": "5.1.1",
    "dotenv": "16.3.1",
    "express": "4.18.2",
    "fast-glob": "3.3.1",
    "jsonwebtoken": "9.0.2",
    "mongodb": "6.2.0",
    "validator": "13.11.0"
  }
