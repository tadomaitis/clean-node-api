    "jest": "27.4.7",
    "lint-staged": "12.1.5",
    "standard": "16.0.4",
    "supertest": "6.1.6"
  },
  "dependencies": {
    "bcrypt": "5.0.1",
    "dotenv": "10.0.0",
    "express": "4.17.2",
    "fast-glob": "3.2.7",
    "jsonwebtoken": "^8.5.1",
    "mongodb": "4.2.2",
    "validator": "^13.7.0"
